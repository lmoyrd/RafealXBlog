{
	"name": "gsp",
	"description": "Grunt task management for Zend Framework 2",
	"version": "2.4.9",
	"license": "GPL-3.0",
	"author":
	{
		"name": "Henry Ruhs",
		"url": "http://redaxmedia.com"
	},
	"bugs":
	{
		"url": "http://github.com/redaxmedia/gsp/issues"
	},
	"dependencies":
	{
		"grunt": "0.4.5",
		"grunt-autoprefixer": "3.0.3",
		"grunt-colorguard": "0.2.0",
		"grunt-contrib-csslint": "0.5.0",
		"grunt-contrib-cssmin": "0.14.0",
		"grunt-contrib-jshint": "0.11.3",
		"grunt-contrib-less": "1.1.0",
		"grunt-contrib-qunit": "0.7.0",
		"grunt-contrib-sass": "0.9.2",
		"grunt-contrib-uglify": "0.11.0",
		"grunt-contrib-watch": "0.6.1",
		"grunt-cssbeautifier": "0.1.9",
		"grunt-htmlhint": "0.9.12-fix",
		"grunt-img": "0.1.8",
		"grunt-jsbeautifier": "0.2.10",
		"grunt-jsonlint": "1.0.7",
		"grunt-phpcs": "0.4.0",
		"grunt-phpunit": "0.3.6",
		"grunt-lineending": "0.2.4",
		"grunt-shell": "1.1.2",
		"grunt-smushit": "1.3.0",
		"grunt-stripmq": "0.0.6",
		"grunt-svgmin": "3.1.2"
	},
	"scripts":
	{
		"test": "grunt --disabled=phpcs --verbose"
	},
	"setup":
	{
		"autoprefixer":
		{
			"name": "grunt-autoprefixer",
			"file": "autoprefixer.json",
			"register":
			{
				"optimize": "autoprefixer"
			}
		},
		"cssbeautifier":
		{
			"name": "grunt-cssbeautifier",
			"file": "cssbeautifier.json",
			"register":
			{
				"beautify": "cssbeautifier",
				"optimize": "cssbeautifier"
			}
		},
		"csslint":
		{
			"name": "grunt-contrib-csslint",
			"file": "csslint.json",
			"register":
			{
				"default": "csslint"
			}
		},
		"cssmin":
		{
			"name": "grunt-contrib-cssmin",
			"file": "cssmin.json",
			"register":
			{
				"build": "cssmin"
			}
		},
		"colorguard":
		{
			"name": "grunt-colorguard",
			"file": "colorguard.json",
			"register":
			{
				"optimize": "colorguard"
			}
		},
		"htmlhint":
		{
			"name": "grunt-htmlhint",
			"file": "htmlhint.json",
			"register":
			{
				"default": "htmlhint"
			}
		},
		"img":
		{
			"name": "grunt-img",
			"file": "img.json",
			"register":
			{
				"imgmin": "img",
				"optimize": "img"
			}
		},
		"jsbeautifier":
		{
			"name": "grunt-jsbeautifier",
			"file": "jsbeautifier.json",
			"register":
			{
				"beautify": "jsbeautifier",
				"optimize": "jsbeautifier"
			}
		},
		"jshint":
		{
			"name": "grunt-contrib-jshint",
			"file": "jshint.json",
			"register":
			{
				"default": "jshint"
			}
		},
		"jsonlint":
		{
			"name": "grunt-jsonlint",
			"file": "jsonlint.json",
			"register":
			{
				"default": "jsonlint"
			}
		},
		"less":
		{
			"name": "grunt-contrib-less",
			"file": "less.json"
		},
		"lineending":
		{
			"name": "grunt-lineending",
			"file": "lineending.json",
			"register":
			{
				"eol": "lineending",
				"optimize": "lineending"
			}
		},
		"phpcs":
		{
			"name": "grunt-phpcs",
			"file": "phpcs.json",
			"register":
			{
				"default": "phpcs"
			}
		},
		"phpunit":
		{
			"name": "grunt-phpunit",
			"file": "phpunit.json",
			"register":
			{
				"test": "phpunit"
			}
		},
		"qunit":
		{
			"name": "grunt-contrib-qunit",
			"file": "qunit.json",
			"register":
			{
				"test": "qunit"
			}
		},
		"sass":
		{
			"name": "grunt-contrib-sass",
			"file": "sass.json"
		},
		"shell":
		{
			"name": "grunt-shell",
			"file": "shell.json",
			"register":
			{
				"optimize": "toc",
				"toc": "shell:toc",
				"tocforce": "shell:tocforce",
				"toclint": "shell:toclint"
			}
		},
		"smushit":
		{
			"name": "grunt-smushit",
			"file": "smushit.json",
			"register":
			{
				"imgmin": "smushit",
				"optimize": "smushit"
			}
		},
		"stripmq":
		{
			"name": "grunt-stripmq",
			"file": "stripmq.json",
			"register":
			{
				"build": "stripmq"
			}
		},
		"svgmin":
		{
			"name": "grunt-svgmin",
			"file": "svgmin.json",
			"register":
			{
				"imgmin": "svgmin",
				"optimize": "svgmin",
				"svgo": "svgmin"
			}
		},
		"uglify":
		{
			"name": "grunt-contrib-uglify",
			"file": "uglify.json",
			"register":
			{
				"build": "uglify"
			}
		},
		"watch":
		{
			"name": "grunt-contrib-watch",
			"file": "watch.json"
		}
	}
}
